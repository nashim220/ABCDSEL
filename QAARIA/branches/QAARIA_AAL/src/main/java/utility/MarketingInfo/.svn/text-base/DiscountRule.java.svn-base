public class DiscountRule
{
    private String ruleId;
    private String ruleDesc;
    private String discountType;
    private String discountAmt;
    private String ruleNumber;
    private String ruleName;
    private String ruleDurationType;
    private String ruleMaxMonths;
    private String ruleCurrency;
    public DiscountRule()
    {

    }
    public String getRuleId()
    {
        return ruleId;
    }

    public void setRuleId(String ruleId)
    {
        this.ruleId = ruleId;
    }

    public String getRuleDesc()
    {
        return ruleDesc;
    }

    public void setRuleDesc(String ruleDesc)
    {
        this.ruleDesc = ruleDesc;
    }

    public String getDiscountType()
    {
        return discountType;
    }

    public void setDiscountType(String discountType)
    {
        this.discountType = discountType;
    }

    public String getDiscountAmt()
    {
        return discountAmt;
    }

    public void setDiscountAmt(String discountAmt)
    {
        this.discountAmt = discountAmt;
    }

    public String getRuleNumber()
    {
        return ruleNumber;
    }

    public String getRuleCurrency()
    {
        return ruleCurrency;
    }

    public void setRuleCurrency(String ruleCurrency)
    {
        this.ruleCurrency = ruleCurrency;
    }

    public void setRuleNumber(String ruleNumber)
    {
        this.ruleNumber = ruleNumber;
    }

    public String getRuleName()
    {
        return ruleName;
    }

    public void setRuleName(String ruleName)
    {
        this.ruleName = ruleName;
    }

    public String getRuleDurationType()
    {
        return ruleDurationType;
    }

    public void setRuleDurationType(String ruleDurationType)
    {
        this.ruleDurationType = ruleDurationType;
    }

    public String getRuleMaxMonths()
    {
        return ruleMaxMonths;
    }

    public void setRuleMaxMonths(String ruleMaxMonths)
    {
        this.ruleMaxMonths = ruleMaxMonths;
    }

    public void print()
    {
//        private String ruleId;
//        private String ruleDesc;
//        private String discountType;
//        private String discountAmt;

        System.out.println("~~~~~~~~~~~~~~~~~~~Rule~~~~~~~~~~~~~~~~~~~~");
        System.out.println("\tRule No: "+this.ruleNumber);
        System.out.println("\tRule Name: "+this.ruleName);
        System.out.println("\tRule ID: "+this.ruleId);
        System.out.println("\tRule Desc: "+this.ruleDesc);
        System.out.println("\tRule Discount Type: "+this.discountType);
        System.out.println("\tRule Discount Amt: "+this.discountAmt);
        System.out.println("\tRule Max Months: "+this.ruleMaxMonths);
    }
}
