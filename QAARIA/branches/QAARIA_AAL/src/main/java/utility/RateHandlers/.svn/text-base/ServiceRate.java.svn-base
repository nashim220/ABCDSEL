package utility.RateHandlers;

/**
 * Created by joe on 3/10/16.
 */
public class ServiceRate
{
    private String clientRateScheduleId;
    private double ratePerUnit;
    private String currency;
    public ServiceRate()
    {

    }

    public String getClientRateScheduleId()
    {
        return clientRateScheduleId;
    }

    public void setClientRateScheduleId(String clientRateScheduleId)
    {
        this.clientRateScheduleId = clientRateScheduleId;
    }

    public double getRatePerUnit()
    {
        return ratePerUnit;
    }

    public void setRatePerUnit(double ratePerUnit)
    {
        this.ratePerUnit = ratePerUnit;
    }

    public String getCurrency()
    {
        return currency;
    }

    public void setCurrency(String currency)
    {
        this.currency = currency;
    }
}
