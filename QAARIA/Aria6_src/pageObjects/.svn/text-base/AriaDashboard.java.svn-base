package pageObjects;

import org.openqa.selenium.By;

/**
 * Created by joseph.gannon on 4/23/2015.
 */
public class AriaDashboard
{
    public static By getDashboardTitle()
    {
        return By.id("title");
    }
    public static By logout()
    {
        return By.linkText("Log Out");
    }

    public static By clickNavBarAccounts()
    {
        return By.partialLinkText("Accounts");
    }
    public static By clickSearch()
    {
        return By.xpath("//*[@id=\'ui-accordion-1-panel-1\']/li[1]/a/div");
    }
    public static By clickRecentRegistration()
    {
        return By.xpath("//*[@id=\'ui-accordion-1-panel-1\']/li[2]/ul/li[5]/a");
    }
    public static By clickAdHocSearch()
    {
        return By.xpath("//*[@id=\'ui-accordion-1-panel-1\']/li[2]/ul/li[1]/a/span");
    }
    public static By getProducts()
    {
        return By.xpath("//*[@id=\"ui-accordion-1-header-2\"]/a/div");
    }
    public static By getServices()
    {
        return By.xpath("//*[@id=\"ui-accordion-1-panel-2\"]/li[7]/a/div");
    }

}
